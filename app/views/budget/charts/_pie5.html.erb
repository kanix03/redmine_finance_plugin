$(function() {	
$('#pie5').highcharts({
        chart: {
            plotBackgroundColor: null,
            plotBorderWidth: null,
            plotShadow: false
        },
        title: {
            text: '<%= l(:chart_real_income_by_projects) %>'
        },
        tooltip: {
    	    pointFormat: '{point.name}: <b>{point.percentage:.1f}%</b> - ${point.m}'
        },
        plotOptions: {
            pie: {
                allowPointSelect: true,
                cursor: 'pointer',
                dataLabels: {
                    enabled: true,
                    color: '#000000',
                    connectorColor: '#000000',
                    format: '<b>{point.name}</b>:<br> {point.percentage:.1f} % - ${point.m}'
                },showInLegend: true
            }
        },
        series: [{
            type: 'pie',
            name: '<%= l(:chart_real_income_by_projects) %>',
            data: [
                <% sum = @sums_array.map {|s| s[6]}.reduce(0, :+)%><% @sums_array.each do |v| %><% unless v[0] == "pr" %>{
                    name: '<%=v[2]%>',
                    y: <%=v[5]/sum*100%>,
		    m: <%=v[5]%>,
                },<%end%><%end%>
            ]
        }]
    });
});
